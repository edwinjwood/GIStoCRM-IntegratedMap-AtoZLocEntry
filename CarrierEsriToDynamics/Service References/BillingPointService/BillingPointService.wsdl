    <wsdl:operation name="GetBillingPointProfile">
      <wsdl:documentation>	&lt;summary&gt;
 Retrieve the billing point profile information for the given billing point id.
 &lt;/summary&gt;
	&lt;param name="billingPointID"&gt;The billing point for the profile you wish to retrieve&lt;/param&gt;
	&lt;returns&gt;&lt;/returns&gt;
	&lt;remarks&gt;&lt;/remarks&gt;</wsdl:documentation>
      <wsdl:input wsaw:Action="http://hickorytech.com/suitesolution/client/service/IBillingPointServiceRest/GetBillingPointProfile" message="tns:IBillingPointService_GetBillingPointProfile_InputMessage" />
      <wsdl:output wsaw:Action="http://hickorytech.com/suitesolution/client/service/IBillingPointServiceRest/GetBillingPointProfileResponse" message="tns:IBillingPointService_GetBillingPointProfile_OutputMessage" />
    </wsdl:operation>
    <wsdl:operation name="AddBillingPointSubmit">
      <wsdl:documentation>	&lt;summary&gt;
 Add a new billing point including profile, account, credit detail, alternate address, deposit, payment details, and tax exemption data.
 &lt;example&gt;&lt;code&gt;

Dim userID As Integer = 4352

Using bpClient = ClientBuilder.BuildClient(Of BillingPointClient)()

    ' get the list of available states
    Dim states As List(Of String) = bpClient.GetAvailableStates()

    ' build the add billing point object
    Dim addBPData As New AddBillingPointData

    ' set the profile for the new billing point
    With addBPData.NewBillingPoint.Profile
        ' business and/or first/last name is required
        .Name.BusinessName = ""
        .Name.FirstName = "Test"
        .Name.LastName = "Customer"
        ' mailing address is required
        .MailingAddress.Attn = False
        .MailingAddress.Street = "123 Test Street" ' required
        .MailingAddress.Street2 = "" ' street2 is required if Attn = True
        .MailingAddress.Street3 = "" ' optional
        .MailingAddress.City = "Mankato" ' required
        .MailingAddress.State = states(0) ' required, must be a value from the available states
        .MailingAddress.ZipCode = "35345" ' required
        .MailingAddress.County = ""
        .MailingAddress.Country = ""
        ' remaining optional profile data
        .HomePhone = "9896789999"
        .WorkPhone = "9897432345"
        .FaxNumber = "9897763456"
        .EmailAddress = "test@test.com"
        .DriversLicenseNumber = "DL4352354234"
        .DriversLicenseState = states(0) ' must be a value from the available states
        .Employer = "Test Company"
        .Occupation = "Tech Services"
        .SSN = "999999999"
        .DateOfBirth = Nothing
        .AuthorizedPassword = "my password"
        .AuthorizedPasswordHint = "your password"
        .AdditionalUser = ""
        .AdditionalUserPassword = ""
        .AdditionalUserPasswordHint = ""
    End With

    ' determines if external address validation should be bypassed, False by default
    addBPData.BypassExternalAddressValidation = False

    ' optionally set the customer ID, set to zero if an existing customer ID is not known
    addBPData.NewBillingPoint.CustomerID = 0

    ' set the date when the billing point is active
    addBPData.ServiceDate = Date.Today

    ' set the add billing point reason code from the list of available options if required
    Dim reasonCodes As Dictionary(Of Integer, String) = bpClient.GetAddBillingPointReasonCodes()
    addBPData.ReasonCode = reasonCodes.First.Key

    ' optionally set the security role required to access the new billing point
    Dim securityRoles As Dictionary(Of Integer, String) = bpClient.GetAvailableSecurityRoles(userID)
    addBPData.RequiredSecurityRoleID = securityRoles.First.Key

    ' optionally set the media type for the new billing point
    Dim mediaTypes As Dictionary(Of Integer, String) = bpClient.GetAvailableMediaTypes()
    addBPData.NewBillingPoint.MediaType = mediaTypes.First.Key

    ' set other exemption, auto-restore, and address data
    With addBPData.NewBillingPoint
        .EmiID = "EMITST" ' optional
        .Profile.MailingAddress.GeoCode = "TestGeo" ' optional
    End With

    'Set Account Invoice Options
    With addBPData.NewBillingPoint.InvoiceOptions
        .AccountCodeInvoiceFormat = 1 ' optional
        .AccountCodeInvoicePrintFormat = "TST" ' optional
        .InvoicePullCode = "TST"
        .AdditionalInvoiceCopies = 1 ' optional
    End With

    'Add Billing point account info data
    ' get the available bill groups based on account and address data and set the new billing point's
    ' bill groups to the value in the list
    Dim billGroups As List(Of BillGroupData) = bpClient.GetAvailableBillGroups(userID,
                                                                               addBPData.NewBillingPoint.AccountInfo.AccountTypeID,
                                                                               DirectCast(addBPData.NewBillingPoint.Profile.MailingAddress, AddressData))

    Dim defaultBillGroup As BillGroupData = billGroups(0)
    addBPData.NewBillingPoint.AccountInfo.BillGroupID = defaultBillGroup.BillGroupID

    ' optionally set the agent for the new billing point
    Dim agents As List(Of AgentData) = bpClient.GetAvailableAgents(userID)
    addBPData.NewBillingPoint.AccountInfo.CommissionAgent = agents(0).AgentID

    addBPData.NewBillingPoint.AccountInfo.LateChargeExempt = False
    addBPData.NewBillingPoint.AccountInfo.DisconnectExempt = False
    addBPData.NewBillingPoint.AccountInfo.PreventAutoRestore = False
    addBPData.NewBillingPoint.AccountInfo.ElectronicPaymentAvailibility = ElectronicPaymentAvailibilityTypeData.AcceptAllPayments ' optional

    ' get the available account types and set the new billing point's account type to the first value
    Dim accountTypes As Dictionary(Of Integer, String) = bpClient.GetAvailableAccountTypes()
    addBPData.NewBillingPoint.AccountInfo.AccountTypeID = accountTypes.First.Key

    ' get the available tax exemptions
    Dim exemptions As Dictionary(Of Integer, String) = Nothing
    Using ctrlDataClient = ClientBuilder.BuildClient(Of ControlDataClient)()
        exemptions = ctrlDataClient.GetAvailableTaxExemptions()
    End Using

    ' add optional tax exemptions
    addBPData.TaxExemptions.Add(exemptions.First.Key)
    addBPData.TaxExemptions.Add(exemptions.Last.Key)

    ' get invoice suppression reasons
    Dim suppressBillViewReasons As Dictionary(Of Integer, String) = bpClient.GetBillViewSuppressionReasons()
    Dim suppressPrintFileReasons As Dictionary(Of Integer, String) = bpClient.GetPrintFileSuppressionReasons()

    ' set optional invoice suppression
    With addBPData.NewBillingPoint.InvoiceSuppression
        .SuppressInvoiceBillView = True
        .SuppressInvoiceBillViewReason = suppressBillViewReasons.First.Key ' required if SuppressInvoiceBillView = True
        .SuppressInvoicePrintFile = True
        .SuppressInvoicePrintFileReason = suppressPrintFileReasons.First.Key ' required if SuppressInvoicePrintFile = True
    End With

    ' get credit rating, credit card type, direct debit account types, direct debit prenote types, and deposit data
    Dim ratings As List(Of String) = Nothing
    Dim creditCardTypes As List(Of CreditCardTypeData) = Nothing
    Dim directDebitAccountTypes As Dictionary(Of Integer, String) = Nothing
    Dim directDebitPrenoteTypes As Dictionary(Of Integer, String) = Nothing
    Dim depositSettings As DepositSettingsData = Nothing
    Dim depositTypes As Dictionary(Of Integer, String) = Nothing
    Dim depositCharges As Dictionary(Of Integer, String) = Nothing
    Using pmtClient = ClientBuilder.BuildClient(Of PaymentClient)()
        ' the available credit ratings need to be fetched based on the billing point's address and other data
        ratings = pmtClient.GetAvailableCreditRatings(addBPData.NewBillingPoint.AccountInfo.AccountTypeID,
                                                      DirectCast(addBPData.NewBillingPoint.Profile.MailingAddress, AddressData),
                                                      addBPData.NewBillingPoint.AccountInfo.BillGroupID,
                                                      defaultBillGroup.ProductCategory)
        ' get payment method data
        creditCardTypes = pmtClient.GetCreditCardTypes()
        directDebitAccountTypes = pmtClient.GetDirectDebitAccountTypes()
        directDebitPrenoteTypes = pmtClient.GetDirectDebitPrenoteTypes()
        ' get deposit settings and data
        depositSettings = pmtClient.GetDepositSettings()
        depositTypes = pmtClient.GetDepositTypes()
        depositCharges = pmtClient.GetAvailableDepositCharges(addBPData.NewBillingPoint.AccountInfo.AccountTypeID,
                                                              DirectCast(addBPData.NewBillingPoint.Profile.MailingAddress, AddressData),
                                                              addBPData.NewBillingPoint.AccountInfo.BillGroupID,
                                                              defaultBillGroup.ProductCategory)

    End Using

    ' set payment method
    addBPData.NewBillingPoint.PaymentMethod = PaymentTypeData.CashCheck

    ' add credit card data if the credit card payment method is selected
    If (addBPData.NewBillingPoint.PaymentMethod = PaymentTypeData.CreditCard) Then
        addBPData.CreditCardAccount = New CreditCardAccountData()
        ' all data is required
        With addBPData.CreditCardAccount
            .CardHolderName = "Test Customer"
            .CardTypeID = creditCardTypes(0).CreditCardID ' must be a value from the available card types
            .Number = "12234234124123" ' length is determined by the selected card type
            .ExpirationMonth = "05" ' month 01-12
            .ExpirationYear = "2016" ' four-digit year
        End With
    ElseIf (addBPData.NewBillingPoint.PaymentMethod = PaymentTypeData.DirectDebit) Then
        ' add direct debit data if the direct debit payment method is selected
        addBPData.DirectDebitAccount = New DirectDebitAccountData()
        With addBPData.DirectDebitAccount
            .AccountName = "My bank account" ' optional
            .AccountNumber = "2452345234" ' required
            .AccountTypeID = directDebitAccountTypes.First.Key ' required, must be a value from the available direct debit types
            .PrenoteID = directDebitPrenoteTypes.First.Key ' required, must be a value from the available prenote types
            .RoutingNumber = "333444555" ' required
        End With
    End If

    ' set the required credit details data
    With addBPData.CreditDetails
        .Agency = "ABCAgency" ' optional
        .AssessmentDate = Date.Today ' required
        .Notes = "This agency is very accurate" 'optional
        .Rating = ratings(0) ' required
        .Score = "2423" 'optional
    End With

    ' add optional deposit data
    addBPData.Deposit = New DepositData()
    With addBPData.Deposit
        .Amount = 45 ' required dollar amount that is greater than zero
        .DepositDate = Date.Today ' required
        .DepositType = depositTypes.First.Key ' required
        .Notes = "This is the initial deposit" ' optional
        ' if deposit charge selection is allowed
        If (depositSettings.BillingChargeAvailibilityType &amp;lt;&amp;gt; DepositBillChargeAvailibilityTypeData.NotAvailable) Then
            ' default the charge to the first available deposit charge
            .ChargeID = depositCharges.First.Key
            ' if there is a default deposit type if a charge is selected
            If (depositSettings.BillingChargeDepositTypeID.HasValue()) Then
                .DepositType = depositSettings.BillingChargeDepositTypeID.Value
            End If
        End If
        ' optionally set the refund date if enabled
        If (depositSettings.IsScheduledRefundDateEnabled) Then
            .RefundDate = Date.Today.AddDays(30) ' optional
        End If
    End With

    ' submit the add billing point request
    Dim addBPStatus As AddBillingPointStatusData = bpClient.AddBillingPointSubmit(addBPData)
    Dim newBillingPointID As Long = 0
    Dim errorMessages As New List(Of String)

    ' if the billing point was successfully created get the new billing point ID
    If (addBPStatus.StatusCode = OperationStatusCode.Success) Then
        newBillingPointID = addBPStatus.BillingPointID
    End If

    ' collect error or warning messages
    addBPStatus.Messages.ToList().ForEach(Sub(x) errorMessages.Add(x.Description))

End Using

&lt;/code&gt;&lt;/example&gt;&lt;/summary&gt;
	&lt;param name="bpData"&gt;Contains all data required to create the new billing point.&lt;/param&gt;
	&lt;returns&gt;An object indicating if the billing point was successfully created, validation errors were detected, or 
 an exception occurred while processing the request.  If successful the BillingPointID property will be populated 
 with the new unique ID.&lt;/returns&gt;
	&lt;remarks&gt;The property bpData.NewBillingPoint.Profile.MailingAddress.TaxLocation is ignored and not saved by this method.  The correct
 tax location ID, if any, will be looked up and saved automatically as part of the submit process.  Note that some numeric value must
 still be provided for this property, as it is non-nullable; omitting the element entirely from the SOAP XML contents is also an option.&lt;/remarks&gt;</wsdl:documentation>
      <wsdl:input wsaw:Action="http://hickorytech.com/suitesolution/client/service/IBillingPointService/AddBillingPointSubmit" message="tns:IBillingPointService_AddBillingPointSubmit_InputMessage" />
      <wsdl:output wsaw:Action="http://hickorytech.com/suitesolution/client/service/IBillingPointService/AddBillingPointSubmitResponse" message="tns:IBillingPointService_AddBillingPointSubmit_OutputMessage" />
    </wsdl:operation>
